<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>RemoveASPNETheaders</id>
        <version>1.0.0.2</version>
        <title>Remove ASP.NET headers</title>
        <authors>Nimesh Manmohanlal</authors>
        <owners>Nimesh Manmohanlal</owners>
        <licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
        <projectUrl>https://github.com/nimeshjm/RemoveASPNETheaders</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>This package will reduce the server information that is disclosed to your visitors.
      It removes the following http headers:
      "Server"
      "X-AspNet-Version"
      "X-AspNetMvc-Version"
      "X-Powered-By"</description>
        <summary>This package will reduce the server information that is disclosed to your visitors.</summary>
        <copyright>Digital Technologies Ltd.</copyright>
        <language>en-GB</language>
        <tags>security, pentest, asp.net, headers</tags>
        <dependencies>
            <group targetFramework=".NETFramework4.0">
                <dependency id="Microsoft.AspNet.Mvc" version="5.0.1" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="web.config.install.xdt" target="content\web.config.install.xdt" />
        <file src="bin\Release20\DigitalTechnologies.RemoveHeaders.dll" target="lib\net20\DigitalTechnologies.RemoveHeaders.dll" />
        <file src="bin\Release45\DigitalTechnologies.RemoveHeaders.dll" target="lib\net45\DigitalTechnologies.RemoveHeaders.dll" />
    </files>
</package>